{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["react-app", "prettier", "react-app/jest", "plugin:css-modules/recommended"],
    "plugins": ["react", "prettier", "css-modules", "import"],
    "rules": {
      "curly": ["off"],
      "no-shadow": "off",
      "no-param-reassign": ["error", { "props": false }],
      "react-hooks/exhaustive-deps": "off",
      "no-underscore-dangle": "off",
      "no-console": "off",
      "no-unused-vars": "off",
      "no-multiple-empty-lines": ["error", { "max": 2 }],
      "css-modules/no-unused-class": [1, { "camelCase": true }],
      "css-modules/no-undef-class": [1, { "camelCase": true }],
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ],
      "no-relative-import-paths/no-relative-import-paths": "off"
    }
  }
{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app", "plugin:prettier/recommended"],
  "plugins": ["react", "prettier"],
  "rules": {
    "curly": ["off"],
    "no-shadow": "off",
    "no-underscore-dangle": "off",
    "no-console": "warn",
    "no-unused-vars": "warn",
    "no-multiple-empty-lines": ["error", { "max": 1 }],
    "no-relative-import-paths/no-relative-import-paths": "off"
  }
}
